openapi: 3.0.0
info:
  title: MyTimeAPI
  version: '2.0'
  description: REST and RPC API to work with the CATS Time Entry system.
  contact:
    email: PHPDev_Group@ftr.com
servers:
  - url: 'http://mytime.opperp.nwestnetwork.com'
    description: OPPERP
paths:
  '/v2/profile/{cris}':
    parameters:
      - type: string
        name: cris
        in: path
        required: true
        description: MyTime user CRIS ID
        schema:
          type: string
          pattern: '^[0-9]{5,6}$'
          minLength: 5
          maxLength: 6
    get:
      summary: GetProfile - PutProfile
      tags:
        - Users
      responses:
        '200':
          description: OK
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
        '500':
          description: Internal Server Error
          content:
            application/json:
              schema:
                title: Profile
                type: object
                description: User MyTime profile
                properties:
                  message:
                    type: string
                  cris:
                    type: string
                    minLength: 6
                    maxLength: 6
                    pattern: '^[0-9]{6}$'
                  name:
                    type: string
                    description: User name in CATS
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: 3 digit site code
                  workgroup:
                    type: string
                    description: CATS workgroup code
                  workgroup_status:
                    type: string
                    description: Current workgroup status in CATS
                  home_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: State abbreviation
                  alt1_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: User alternate state 1
                  calccode:
                    type: string
                    description: Calculation code
                  alt2_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: User alternate state 2
                  workday4:
                    type: string
                    description: Scheduled flag for Wednesday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday3:
                    type: string
                    description: Scheduled flag for Tuesday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday2:
                    type: string
                    description: Scheduled flag for Monday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday1:
                    type: string
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                    description: Scheduled flag for Sunday
                  costcenter:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: User cost center in CATS
                  tour:
                    type: string
                    description: User tour in CATS
                    minLength: 1
                    maxLength: 1
                    pattern: '^[A-Z]$'
                  laborclass:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: User labor class from CATS
                  endtime:
                    type: string
                    description: Not Used
                  starttime:
                    type: string
                    description: Not Used
                  workday7:
                    type: string
                    description: Scheduled flag for Saturday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday6:
                    type: string
                    description: Scheduled flag for Friday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday5:
                    type: string
                    description: Scheduled flag for Thursday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  securitySchemes: {}
                  activePeriodEndDate:
                    type: string
                    description: User site active period end date from CATS
                    format: date
                    maxLength: 10
                    pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                  week:
                    type: string
                    format: date
                    maxLength: 10
                    pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                    description: 'For PUT requests, the week the updates should be applied'
                required:
                  - alt1_state
                  - alt2_state
                  - tour
                  - week
      operationId: get-profile
      description: Get user MyTime profile
    put:
      summary: ''
      operationId: put-profile
      responses:
        '200':
          description: OK
        '400':
          description: Bad Request
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
        '422':
          description: Unprocessable Entity
        '500':
          description: Internal Server Error
          content:
            application/json:
              schema:
                title: Profile
                type: object
                description: User MyTime profile
                properties:
                  message:
                    type: string
                  cris:
                    type: string
                    minLength: 6
                    maxLength: 6
                    pattern: '^[0-9]{6}$'
                  name:
                    type: string
                    description: User name in CATS
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: 3 digit site code
                  workgroup:
                    type: string
                    description: CATS workgroup code
                  workgroup_status:
                    type: string
                    description: Current workgroup status in CATS
                  home_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: State abbreviation
                  alt1_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: User alternate state 1
                  calccode:
                    type: string
                    description: Calculation code
                  alt2_state:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z]{2}$'
                    description: User alternate state 2
                  workday4:
                    type: string
                    description: Scheduled flag for Wednesday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday3:
                    type: string
                    description: Scheduled flag for Tuesday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday2:
                    type: string
                    description: Scheduled flag for Monday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday1:
                    type: string
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                    description: Scheduled flag for Sunday
                  costcenter:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: User cost center in CATS
                  tour:
                    type: string
                    description: User tour in CATS
                    minLength: 1
                    maxLength: 1
                    pattern: '^[A-Z]$'
                  laborclass:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: User labor class from CATS
                  endtime:
                    type: string
                    description: Not Used
                  starttime:
                    type: string
                    description: Not Used
                  workday7:
                    type: string
                    description: Scheduled flag for Saturday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday6:
                    type: string
                    description: Scheduled flag for Friday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  workday5:
                    type: string
                    description: Scheduled flag for Thursday
                    minLength: 1
                    maxLength: 1
                    pattern: '^[Y|N]$'
                  securitySchemes: {}
                  activePeriodEndDate:
                    type: string
                    description: User site active period end date from CATS
                    format: date
                    maxLength: 10
                    pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                  week:
                    type: string
                    format: date
                    maxLength: 10
                    pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                    description: 'For PUT requests, the week the updates should be applied'
                required:
                  - alt1_state
                  - alt2_state
                  - tour
                  - week
      description: Update user MyTime profile
      requestBody:
        content:
          application/json:
            schema:
              title: Profile
              type: object
              description: User MyTime profile
              properties:
                message:
                  type: string
                cris:
                  type: string
                  minLength: 6
                  maxLength: 6
                  pattern: '^[0-9]{6}$'
                name:
                  type: string
                  description: User name in CATS
                site:
                  type: string
                  minLength: 3
                  maxLength: 3
                  pattern: '^[0-9]{3}$'
                  description: 3 digit site code
                workgroup:
                  type: string
                  description: CATS workgroup code
                workgroup_status:
                  type: string
                  description: Current workgroup status in CATS
                home_state:
                  type: string
                  minLength: 2
                  maxLength: 2
                  pattern: '^[A-Z]{2}$'
                  description: State abbreviation
                alt1_state:
                  type: string
                  minLength: 2
                  maxLength: 2
                  pattern: '^[A-Z]{2}$'
                  description: User alternate state 1
                calccode:
                  type: string
                  description: Calculation code
                alt2_state:
                  type: string
                  minLength: 2
                  maxLength: 2
                  pattern: '^[A-Z]{2}$'
                  description: User alternate state 2
                workday4:
                  type: string
                  description: Scheduled flag for Wednesday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                workday3:
                  type: string
                  description: Scheduled flag for Tuesday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                workday2:
                  type: string
                  description: Scheduled flag for Monday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                workday1:
                  type: string
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                  description: Scheduled flag for Sunday
                costcenter:
                  type: string
                  minLength: 3
                  maxLength: 3
                  pattern: '^[0-9]{3}$'
                  description: User cost center in CATS
                tour:
                  type: string
                  description: User tour in CATS
                  minLength: 1
                  maxLength: 1
                  pattern: '^[A-Z]$'
                laborclass:
                  type: string
                  minLength: 3
                  maxLength: 3
                  pattern: '^[0-9]{3}$'
                  description: User labor class from CATS
                endtime:
                  type: string
                  description: Not Used
                starttime:
                  type: string
                  description: Not Used
                workday7:
                  type: string
                  description: Scheduled flag for Saturday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                workday6:
                  type: string
                  description: Scheduled flag for Friday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                workday5:
                  type: string
                  description: Scheduled flag for Thursday
                  minLength: 1
                  maxLength: 1
                  pattern: '^[Y|N]$'
                securitySchemes: {}
                activePeriodEndDate:
                  type: string
                  description: User site active period end date from CATS
                  format: date
                  maxLength: 10
                  pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                week:
                  type: string
                  format: date
                  maxLength: 10
                  pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}$'
                  description: 'For PUT requests, the week the updates should be applied'
              required:
                - alt1_state
                - alt2_state
                - tour
                - week
      tags:
        - Users
  '/v2/tour/{tour-id}':
    get:
      summary: GetTour
      tags:
        - Users
      responses:
        '200':
          description: OK
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: Tour
                type: object
                properties:
                  id:
                    type: string
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: CATS site
                  tourcd:
                    type: string
                    minLength: 1
                    maxLength: 1
                    pattern: '^[A-Z]$'
                    description: CATS tour code
                  start:
                    type: string
                    description: Not used
                  stop:
                    type: string
                    description: Not used
                  hours:
                    type: string
                    pattern: "^[0-9]{1,2}\\.[0-9]{2}$"
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                required:
                  - id
                  - site
                  - tourcd
                description: Tour information from CATS
                x-tags:
                  - Codes
      operationId: get-tour
      description: Retrieve single tour object
      parameters: []
    parameters:
      - schema:
          type: string
          pattern: "^[0-9]{3}\\.[A-Z]$"
        name: tour-id
        in: path
        required: true
        description: Unique tour ID
  /v2/tour:
    get:
      summary: GetTourCollection
      tags:
        - Users
      responses:
        '200':
          description: OK
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: TourCollection
                type: object
                properties:
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                  _embedded:
                    type: object
                    properties:
                      tour:
                        type: array
                        items:
                          title: Tour
                          type: object
                          properties:
                            id:
                              type: string
                            site:
                              type: string
                              minLength: 3
                              maxLength: 3
                              pattern: '^[0-9]{3}$'
                              description: CATS site
                            tourcd:
                              type: string
                              minLength: 1
                              maxLength: 1
                              pattern: '^[A-Z]$'
                              description: CATS tour code
                            start:
                              type: string
                              description: Not used
                            stop:
                              type: string
                              description: Not used
                            hours:
                              type: string
                              pattern: "^[0-9]{1,2}\\.[0-9]{2}$"
                            _links:
                              title: SelfLink
                              type: object
                              properties:
                                self:
                                  type: object
                                  properties:
                                    href:
                                      type: string
                                      description: URL to current resource
                                      format: uri
                              description: Resource self URL
                          required:
                            - id
                            - site
                            - tourcd
                          description: Tour information from CATS
                          x-tags:
                            - Codes
                  total_items:
                    type: integer
                x-tags:
                  - Codes
      operationId: get-tour-collection
      description: Get collection of tours for the given site query parameter
      parameters:
        - schema:
            type: string
            maxLength: 3
            minLength: 3
            pattern: '^[0-9]{3}$'
          in: query
          name: site
          description: CATS site code
          required: true
    parameters: []
  '/v2/specialhour/{specialhour-id}':
    get:
      summary: GetSpecialHourCode
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: Specialhour
                type: object
                description: Special Hour codes from CATS
                properties:
                  id:
                    type: string
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: CATS site code
                  code:
                    type: string
                    description: Special Hours code from CATS
                    minLength: 2
                    maxLength: 3
                    pattern: '^[A-Z0-9]{2,3}$'
                  description:
                    type: string
                    description: Special Hour code description from CATS
                  rateflag:
                    type: string
                    description: 'Special rate applied in CATS: Y=User entered, R=From CATS'
                    enum:
                      - 'Y'
                      - R
                    minLength: 0
                    maxLength: 1
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                required:
                  - site
                  - code
                x-tags:
                  - Codes
      operationId: get-specialhour
      description: Retrieve special hour code object
    parameters:
      - schema:
          type: string
          pattern: "^[0-9]{3}\\.[A-Z0-9]{2}$"
        name: specialhour-id
        in: path
        required: true
        description: Unique ID of the special hour code
  /v2/specialhour:
    get:
      summary: GetSpecialHourCollection
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: SpecialHourCollection
                type: object
                description: Collection of Special Hour codes
                x-tags:
                  - Codes
                properties:
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                  _embedded:
                    type: object
                    properties:
                      special_hour:
                        type: array
                        items:
                          title: Specialhour
                          type: object
                          description: Special Hour codes from CATS
                          properties:
                            id:
                              type: string
                            site:
                              type: string
                              minLength: 3
                              maxLength: 3
                              pattern: '^[0-9]{3}$'
                              description: CATS site code
                            code:
                              type: string
                              description: Special Hours code from CATS
                              minLength: 2
                              maxLength: 3
                              pattern: '^[A-Z0-9]{2,3}$'
                            description:
                              type: string
                              description: Special Hour code description from CATS
                            rateflag:
                              type: string
                              description: 'Special rate applied in CATS: Y=User entered, R=From CATS'
                              enum:
                                - 'Y'
                                - R
                              minLength: 0
                              maxLength: 1
                            _links:
                              title: SelfLink
                              type: object
                              properties:
                                self:
                                  type: object
                                  properties:
                                    href:
                                      type: string
                                      description: URL to current resource
                                      format: uri
                              description: Resource self URL
                          required:
                            - site
                            - code
                          x-tags:
                            - Codes
      operationId: get-specialhour-collection
      description: Get collection of special hour codes for the given site query parameter
      parameters:
        - schema:
            type: string
            pattern: '^[0-9]{3}$'
          in: query
          name: site
          description: CATS site code
          required: true
    parameters: []
  '/v2/overtime/{overtime-id}':
    get:
      summary: GetOvertimeCode
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: Overtime
                type: object
                description: Overtime code information
                properties:
                  id:
                    type: string
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: CATS site code
                  code:
                    type: string
                    minLength: 1
                    maxLength: 1
                    pattern: '^[A-Z0-9]$'
                    description: Overtime code from CATS
                  description:
                    type: string
                    description: Overtime code description
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                required:
                  - site
                  - code
                  - description
      operationId: get-overtime
      description: Retrieve single overtime code object
    parameters:
      - schema:
          type: string
          pattern: "^[0-9]{3}\\.[0-9]{1,2}\\.[A-Z]$"
        name: overtime-id
        in: path
        required: true
        description: Overtime code ID
  /v2/overtime:
    get:
      summary: GetOvertimeCollection
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: OvertimeCollection
                type: object
                description: Collection of overtime codes for a site
                properties:
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                  _embedded:
                    type: object
                    properties:
                      overtime:
                        type: array
                        items:
                          title: Overtime
                          type: object
                          description: Overtime code information
                          properties:
                            id:
                              type: string
                            site:
                              type: string
                              minLength: 3
                              maxLength: 3
                              pattern: '^[0-9]{3}$'
                              description: CATS site code
                            code:
                              type: string
                              minLength: 1
                              maxLength: 1
                              pattern: '^[A-Z0-9]$'
                              description: Overtime code from CATS
                            description:
                              type: string
                              description: Overtime code description
                            _links:
                              title: SelfLink
                              type: object
                              properties:
                                self:
                                  type: object
                                  properties:
                                    href:
                                      type: string
                                      description: URL to current resource
                                      format: uri
                              description: Resource self URL
                          required:
                            - site
                            - code
                            - description
      operationId: get-overtime-collection
      description: Get collection of overtime codes for the given site query paramter
      parameters:
        - schema:
            type: string
            pattern: '^[0-9]{3}$'
          in: query
          name: site
          description: CATS site code
          required: true
    parameters: []
  '/v2/reimbursement/{reim-id}':
    get:
      summary: GetReimbursement
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '404':
          description: Not Found
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: Reimbursement
                type: object
                description: Reimbursement code infomation from CATS
                properties:
                  id:
                    type: string
                  site:
                    type: string
                    minLength: 3
                    maxLength: 3
                    pattern: '^[0-9]{3}$'
                    description: CATS site code
                  code:
                    type: string
                    minLength: 2
                    maxLength: 2
                    pattern: '^[A-Z0-9]{2}$'
                    description: Reimbursement code from CATS
                  description:
                    type: string
                    description: Reimbursement code description
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                required:
                  - site
                  - code
                  - description
      operationId: get-reimbursement
      description: Retrieve single reimbursement object
    parameters:
      - schema:
          type: string
          pattern: "^[0-9]{3}\\.[A-Z0-9]{2}$"
        name: reim-id
        in: path
        required: true
        description: Reimbursement code
  /v2/reimbursement:
    get:
      summary: GetReimbursementCollection
      tags:
        - Codes
      responses:
        '200':
          description: OK
        '406':
          description: Not Acceptable
        '415':
          description: Unsupported Media Type
          content:
            application/json:
              schema:
                title: ReimbursementCollection
                type: object
                description: Collection of reimbursement codes for a site
                properties:
                  _links:
                    title: SelfLink
                    type: object
                    properties:
                      self:
                        type: object
                        properties:
                          href:
                            type: string
                            description: URL to current resource
                            format: uri
                    description: Resource self URL
                  _embedded:
                    type: object
                    properties:
                      reimbursement:
                        type: array
                        items:
                          title: Reimbursement
                          type: object
                          description: Reimbursement code infomation from CATS
                          properties:
                            id:
                              type: string
                            site:
                              type: string
